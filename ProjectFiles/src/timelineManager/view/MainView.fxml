<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1200.0" styleClass="background-right" stylesheets="@../css/stylesheet.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="200.0" styleClass="background-left" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXDatePicker fx:id="mainWindowDatePicker" defaultColor="#f28026" editable="false" layoutX="20.0" layoutY="95.0" onAction="#setDate" prefHeight="33.0" prefWidth="177.0" promptText="Go to Date" showWeekNumbers="true" />
            <JFXButton fx:id="todaysDateButton" buttonType="RAISED" layoutX="14.0" layoutY="30.0" onAction="#resetDate" prefHeight="27.0" prefWidth="164.0" text="Today" />
            <JFXButton fx:id="addTimelineButton" buttonType="RAISED" layoutX="14.0" layoutY="239.0" onAction="#openAddTimelineWindow" prefHeight="27.0" prefWidth="164.0" text="Add Timeline" />
              <JFXButton fx:id="addTaskButton" buttonType="RAISED" layoutX="14.0" layoutY="292.0" onAction="#openAddTaskWindow" prefHeight="27.0" prefWidth="164.0" text="Add Task" />
            <TableView fx:id="timelineTable" layoutY="379.0" prefHeight="201.0" prefWidth="200.0" styleClass="background-right">
              <columns>
                <TableColumn fx:id="titleOfTable" prefWidth="199.0" text="C1" />
              </columns>
            </TableView>


         </children>
      </AnchorPane>
      <AnchorPane layoutX="200.0" prefHeight="600.0" prefWidth="1001.0">
         <children>
            <StackPane prefHeight="600.0" prefWidth="1001.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="background-right">
                     <children>
                       <JFXButton fx:id="addTaskPlusButton" buttonType="RAISED" layoutX="24.0" layoutY="511.0" onAction="#openAddTaskWindow" prefHeight="65.0" prefWidth="59.0" styleClass="button-round" text="+" />
                        <JFXButton fx:id="goLeftButton" buttonType="RAISED" onAction="#goLeft" styleClass="button-chooser" text="&lt;" AnchorPane.bottomAnchor="286.0" AnchorPane.rightAnchor="935.0" AnchorPane.topAnchor="287.0" />
                        <JFXButton fx:id="goRightButton" buttonType="RAISED" onAction="#goRight" styleClass="button-chooser" text="&gt;" AnchorPane.bottomAnchor="272.0" AnchorPane.leftAnchor="934.0" AnchorPane.topAnchor="301.0" />
                     </children>
                  </AnchorPane>
                  <VBox prefHeight="200.0" prefWidth="100.0" styleClass="background">
                     <children>
                        <GridPane fx:id="dateGrid" styleClass="background">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                        </GridPane>
                        <ScrollPane fx:id="timelineScrollPane" hbarPolicy="NEVER" pannable="true" prefHeight="514.0" prefWidth="634.0" styleClass="background" vbarPolicy="ALWAYS" VBox.vgrow="ALWAYS">
                           <content>
                              <GridPane fx:id="timelineGrid">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                              </GridPane>
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
               </children>
            </StackPane>

         </children>
      </AnchorPane>
   </children>
</AnchorPane>
